<!DOCTYPE html>
<html>
<head>
  <title>Dream</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <section>
    <div class="super-header js-header">
      <div class="container-fluid">
        <div class="row">
          <nav id="main_navbar" class="navbar">
            <div class="navbar-header">
                <div class="logo">
                    <a class="navbar-brand" style="float: none;" href="/">
                      <img alt="Dream Cloud" src="../logo.png">
                    </a>
                  </div>
                </div>
                <div id="userbar" class="bloque_login pull-right">
                  <ul class="menu_cabecera_login">
                    <li class="entra menu_cabecera_login_md ">
                      <a href="#">
                        <i class="icon-AnonymousAvatar"></i>
                        Login
                      </a>
                    </li>
                    <li class="registrate menu_cabecera_login_md ">
                      <a href="#">
                        <i class="icon-NegRatingStarFull"></i>
                          Sign up
                      </a>
                    </li>
                  </ul>
                </div>
                <div id="navbar" class="text-center nav-holder">
                  <div class="menu_cabecera">
                    <ul class="nav navbar-nav">
                      <li>
                        <a href="#"> Dreams</a>
                      </li>
                      <li>
                        <a href="#"> Dictionary</a>
                      </li>
                      <li>
                        <a href="#"> Blog</a>
                      </li>
                   </ul>
                </div>
            </div>
          </nav>
        </div>
      </div>
    </div>
<div class="clase_separador_menu"></div>
</section>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<div class="container">
<%= yield %>

</div>



</body>
</html>
